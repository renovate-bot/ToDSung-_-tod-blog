    "@storybook/addon-webpack5-compiler-swc": "1.0.6",
    "@storybook/blocks": "8.6.11",
    "@storybook/preview-api": "8.6.11",
    "@storybook/react": "8.6.11",
    "@storybook/react-webpack5": "8.6.11",
    "@storybook/test": "8.6.11",
    "@storybook/test-runner": "0.22.0",
